generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Matchs {
  id        Int    @id @default(autoincrement())
  categorie String
  equipe1   String
  equipe2   String
  score1    Int?
  score2    Int?

  @@unique([categorie, equipe1, equipe2])
}
